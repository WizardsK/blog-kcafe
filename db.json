6","content":"<div class=\"linkslist\">\n  <p class=\"asidetitle\"><a class=\"asidetitle\" href=\"#\"><%= __('links') %></a></p>\n    <ul>\n      <li><a href=\"#\" title=\"sdvdxl\">DuCafe</a></li>\n    </ul>\n</div>\n","mtime":1402738017000,"source":"themes/Tinny/layout/_widget/links.ejs"},"wt2aqu7hj2us1qxf":{"_id":"wt2aqu7hj2us1qxf","content":"<% if (site.posts.length){ %>\n  <div class=\"widget-wrap\">\n    <h3 class=\"widget-title\">Recents</h3>\n    <div class=\"widget\">\n      <ul>\n        <% site.posts.sort('date', -1).limit(5).each(function(post){ %>\n          <li>\n            <a href=\"<%- config.root %><%- post.path %>\"><%= post.title || '(no title)' %></a>\n          </li>\n        <% }) %>\n      </ul>\n    </div>\n  </div>\n<% } %>","mtime":1402659139000,"source":"themes/Tinny/layout/_widget/recent_posts.ejs"},"rdg7hw9nx8mkn0l2":{"_id":"rdg7hw9nx8mkn0l2","content":"<div class=\"rsspart\">\n\t<a href=\"<%= theme.rss %>\" target=\"_blank\" title=\"rss\"><%= __('rss') %></a>\n</div>","mtime":1402659139000,"source":"themes/Tinny/layout/_widget/rss.ejs"},"qpjznp3gr58g6chp":{"_id":"qpjznp3gr58g6chp","content":"<% if (site.tags.length){ %>\n<div class=\"tagslist\">\n\t<p class=\"asidetitle\"><%= __('tags') %></p>\n\t\t<ul class=\"clearfix\">\n\t\t<% site.tags.sort('name').limit(20).each(function(item){ %>\n\t\t\t<li><a href=\"<%- config.root %><%- item.path %>\" title=\"<%= item.name %>\"><%= item.name %><sup><%= item.posts.length %></sup></a></li>\n\t\t<% }); %>\n\t\t</ul>\n</div>\n<% } %>","mtime":1402659139000,"source":"themes/Tinny/layout/_widget/tag.ejs"},"qthex31ns9xvf0wf":{"_id":"qthex31ns9xvf0wf","content":"<% if (site.tags.length){ %>\n  <div class=\"tagcloudlist\">\n    <p class=\"asidetitle\"><%= __('tagcloud') %></p>\n    <div class=\"tagcloudlist clearfix\">\n       <%- tagcloud() %>\n    </div>\n  </div>\n<% } %>","mtime":1402659139000,"source":"themes/Tinny/layout/_widget/tagcloud.ejs"},"3etx0j4npzhma3vv":{"_id":"3etx0j4npzhma3vv","content":"<% if (config.disqus_shortname){ %>\n<script>\n  var disqus_shortname = '<%= config.disqus_shortname %>';\n  <% if (page.permalink){ %>\n  var disqus_url = '<%= page.permalink %>';\n  <% } %>\n  (function(){\n    var dsq = document.createElement('script');\n    dsq.type = 'text/javascript';\n    dsq.async = true;\n    dsq.src = '//go.disqus.com/<% if (page.comments){ %>embed.js<% } else { %>count.js<% } %>';\n    (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);\n  })();\n</script>\n<% } %>\n\n<script src=\"//ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js\"></script>\n\n<% if (theme.fancybox){ %>\n<link rel=\"stylesheet\" href=\"<%- config.root %>fancybox/jquery.fancybox.css\" media=\"screen\" type=\"text/css\">\n<script src=\"<%- config.root %>fancybox/jquery.fancybox.pack.js\"></script>\n<% } %>\n\n<%- js('js/script') %>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/after-footer.ejs"},"8pj1mt1t5mhig0m9":{"_id":"8pj1mt1t5mhig0m9","content":"<script src=\"<%- config.root %>js/jquery-2.1.0.min.js\"></script>\n<script type=\"text/javascript\">\n$(document).ready(function(){ \n  //back to top\n  function backToTop(){\n    var buttonHTML = $(\"<a href=\\\"#top\\\" id=\\\"back-top\\\">\" + \"<span>Back to Top</span></a>\");\n    buttonHTML.appendTo($(\"body\"));\n    var buttonToTop = $(\"#back-top\");\n    // hide #back-top first\n    buttonToTop.hide();\n\n    // fade in #back-top\n    $(function() {\n        $(window).scroll(function() {\n            if ($(this).scrollTop() > 200) {\n                buttonToTop.fadeIn();\n            } else {\n                buttonToTop.fadeOut();\n            }\n        });\n        // scroll body to 0px on click\n        buttonToTop.click(function() {\n            $('body,html').animate({\n                scrollTop: 0\n            }, 800);\n            return false;\n        });\n    });\n  }\n  backToTop();\n\n  $('.navbar').click(function(){\n    $('header nav').toggleClass('shownav');\n  });\n  var myWidth = 0;\n  function getSize(){\n    if( typeof( window.innerWidth ) == 'number' ) {\n      myWidth = window.innerWidth;\n    } else if( document.documentElement && document.documentElement.clientWidth) {\n      myWidth = document.documentElement.clientWidth;\n    };\n  };\n  var m = $('#main'),\n      a = $('#asidepart'),\n      c = $('.closeaside'),\n      ta = $('#toc.toc-aside');\n  $(window).resize(function(){\n    getSize(); \n    if (myWidth >= 1024) {\n      $('header nav').removeClass('shownav');\n    }else\n    {\n      m.removeClass('moveMain');\n      a.css('display', 'block').removeClass('fadeOut');\n      <% if( is_post()&&(page.toc !== false) && theme.toc.aside){ %>\n      $('#toc.toc-aside').css('display', 'none');\n      <% } %>  \n    }\n  });\n\n  var show = true;\n  c.click(function(){\n    if(show == true){\n        a.addClass('fadeOut').css('display', 'none');\n        ta.css('display', 'block').addClass('fadeIn');\n        m.addClass('moveMain');  \n    }else{\n        a.css('display', 'block').removeClass('fadeOut').addClass('fadeIn');     \n        ta.css('display', 'none'); \n        m.removeClass('moveMain');\n        $('#toc.toc-aside').css('display', 'none');\n    }\n    show = !show;\n  });\n});\n</script>\n<% if (is_post()) { %>\n<script type=\"text/javascript\">\n$(document).ready(function(){ \n  var ai = $('.article-content>iframe'),\n      ae = $('.article-content>embed'),\n      t  = $('#toc'),\n      h  = $('article h2')\n      ah = $('article h2'),\n      ta = $('#toc.toc-aside'),\n      o  = $('.openaside'),\n      c  = $('.closeaside');\n  if(ai.length>0){\n    ai.wrap('<div class=\"video-container\" />');\n  };\n  if(ae.length>0){\n   ae.wrap('<div class=\"video-container\" />');\n  };\n  if(ah.length==0){\n    t.css('display','none');\n  }else{\n\n    $(window).scroll(function(){\n      ta.css(\"top\",Math.max(140,240-$(this).scrollTop()));\n    });\n  };\n});\n</script>\n<% } %>\n<% if ((page.layout=='post'||page.layout=='page')&&!theme.jiathis.enable) { %>\n<script type=\"text/javascript\">\n$(document).ready(function(){ \n  var $this = $('.share'),\n      url = $this.attr('data-url'),\n      encodedUrl = encodeURIComponent(url),\n      title = $this.attr('data-title'),\n      tsina = $this.attr('data-tsina');\n  var html = [\n  '<a href=\"#\" class=\"overlay\" id=\"qrcode\"></a>',\n  '<div class=\"qrcode clearfix\"><span>扫描二维码分享到微信朋友圈</span><a class=\"qrclose\" href=\"#share\"></a><strong>Loading...Please wait</strong><img id=\"qrcode-pic\" data-src=\"http://s.jiathis.com/qrcode.php?url=' + encodedUrl + '\"/></div>',\n  '<a href=\"#textlogo\" class=\"article-back-to-top\" title=\"Top\"></a>',\n  '<a href=\"https://www.facebook.com/sharer.php?u=' + encodedUrl + '\" class=\"article-share-facebook\" target=\"_blank\" title=\"Facebook\"></a>',\n  '<a href=\"#qrcode\" class=\"article-share-qrcode\" title=\"QRcode\"></a>',\n  '<a href=\"https://twitter.com/intent/tweet?url=' + encodedUrl + '\" class=\"article-share-twitter\" target=\"_blank\" title=\"Twitter\"></a>',\n  '<a href=\"http://service.weibo.com/share/share.php?title='+title+'&url='+encodedUrl +'&ralateUid='+ tsina +'&searchPic=true&style=number' +'\" class=\"article-share-weibo\" target=\"_blank\" title=\"Weibo\"></a>',\n  '<span title=\"Share to\"></span>'\n  ].join('');\n  $this.append(html);\n  $('.article-share-qrcode').click(function(){\n    var imgSrc = $('#qrcode-pic').attr('data-src');\n    $('#qrcode-pic').attr('src', imgSrc);\n    $('#qrcode-pic').load(function(){\n        $('.qrcode strong').text(' ');\n    });\n  });\n});     \n</script>\n<% } %>\n<% if ((page.layout=='post'||page.layout=='page')&&theme.duoshuo.enable && page.comments){ %>\n<script type=\"text/javascript\">\n  var duoshuoQuery = {short_name:\"<%= theme.duoshuo.short_name %>\"};\n  (function() {\n    var ds = document.createElement('script');\n    ds.type = 'text/javascript';ds.async = true;\n    ds.src = '//static.duoshuo.com/embed.js';\n    ds.charset = 'UTF-8';\n    (document.getElementsByTagName('head')[0] \n    || document.getElementsByTagName('body')[0]).appendChild(ds);\n  })();\n</script> \n<% } %>\n<% if (page.layout=='tags'||page.layout=='categories'){ %>\n<script type=\"text/javascript\">\n$(document).ready(function(){ \n  var list    = $('.archive-title a'),\n      box     = $('.all-list-box'),\n      first   = $('.archive-title a:first-of-type');\n      box.load(first.attr('data-src').toString()+' #archive-page');\n      first.addClass('current');\n  list.each(function(){\n    $(this).click(function(){\n      var listSrc=$(this).attr('data-src').toString()+' #archive-page';\n      box.load(listSrc);\n      list.removeClass('current');\n      $(this).addClass('current');\n     });  \n    });\n  }); \n</script>\n<% } %>\n<% if ((page.layout=='post'||page.layout=='page')&&theme.fancybox){ %>\n<link rel=\"stylesheet\" href=\"<%- config.root %>fancybox/jquery.fancybox.css\" media=\"screen\" type=\"text/css\">\n<script src=\"<%- config.root %>fancybox/jquery.fancybox.pack.js\"></script>\n<script type=\"text/javascript\">\n$(document).ready(function(){ \n  $('.article-content').each(function(i){\n    $(this).find('img').each(function(){\n      if ($(this).parent().hasClass('fancybox')) return;\n      var alt = this.alt;\n      if (alt) $(this).after('<span class=\"caption\">' + alt + '</span>');\n      $(this).wrap('<a href=\"' + this.src + '\" title=\"' + alt + '\" class=\"fancybox\"></a>');\n    });\n    $(this).find('.fancybox').each(function(){\n      $(this).attr('rel', 'article' + i);\n    });\n  });\n  if($.fancybox){\n    $('.fancybox').fancybox();\n  }\n}); \n</script>\n<% } %>\n<%- partial('google_analytics') %>\n","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/after_footer.ejs"},"gq17ahcx1r5ppn2t":{"_id":"gq17ahcx1r5ppn2t","content":"<article class=\"archive-article archive-type-<%= post.layout %>\">\n  <div class=\"archive-article-inner\">\n    <header class=\"archive-article-header\">\n      <%- partial('post/date', {class_name: 'archive-article-date', date_format: 'MMM D'}) %>\n      <%- partial('post/title', {class_name: 'archive-article-title'}) %>\n    </header>\n  </div>\n</article>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/archive-post.ejs"},"v65dzyhn94mvodcj":{"_id":"v65dzyhn94mvodcj","content":"<%\nvar title = '';\nvar icon = '';\nif (page.category){\n  title = page.category;\n  icon = 'category';\n};\nif (page.tag){\n  title = page.tag;\n  icon = 'tag';\n};\nif (page.archive){\n  icon = 'archive';\n  if (page.year) title = page.year+  (page.month ? '/' + page.month : '');\n  else title = __('archive_a');\n};\n%>\n\n<div class=\"archive-title\" >\n  <h2 class=\"<%= icon %>-icon\"><%= title %></h2>\n  <% if(page.archive){ %>\n  <div class=\"archiveslist archive-float clearfix\">\n   <%- list_archives() %>\n </div>\n  <% } %>\n</div>\n<div id=\"main\" class=\"archive-part clearfix\">\n  <div id=\"archive-page\">\n<% page.posts.each(function(item){ %>\n  <%- partial('_partial/article', {item: item, index: true,desc: false}) %>\n<% }); %>\n<% if (page.total > 1){ %>\n  <nav id=\"page-nav\" class=\"clearfix archive-nav\">\n    <%- paginator({\n      prev_text: '&laquo; Prev',\n      next_text: 'Next &raquo;'\n    }) %>\n  </nav>\n<% } %>\n  </div>\n</div>\n","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/archive.ejs"},"91oth3fgfna6p4ip":{"_id":"91oth3fgfna6p4ip","content":"<section class=\"<%= item.layout %>\" itemscope itemprop=\"blogPost\">\n  <article>\n    <h1 itemprop=\"name\">\n      <% if (item.link) { %> \n        <a href=\"<%- item.link %>\" target=\"_blank\"> \n      <% } else{ %>\n        <a href=\"<%- config.root %><%- item.path %>\" title=\"<%= item.title %>\" itemprop=\"url\">\n      <% } %>\n        <%= item.title %>\n      </a>\n    </h1>\n    <% if (desc){ %>\n     <% if(item.description){ %>\n      <p itemprop=\"description\" ><%- item.description %></p>\n      <% } else if(item.excerpt){ %>\n       <p itemprop=\"description\" ><%= strip_html(item.excerpt).replace(/^\\s*/, '').replace(/\\s*$/, '').substring(0, 140) %></p>\n      <% } else {%>\n           <p itemprop=\"description\" ><%= strip_html(item.content).replace(/^\\s*/, '').replace(/\\s*$/, '').substring(0, 140) %></p>\n        <% } %>\n    <% } %>\n    <p class=\"article-more-link\">\n      <time datetime=\"<%= date_xml(item.date) %>\" itemprop=\"datePublished\"><%= item.date.format(config.date_format) %></time>\n      <% if (theme.excerpt_link){ %>\n          <a href=\"<%- config.root %><%- item.path %>\"><%= theme.excerpt_link %>&raquo;</a>\n      <% } %>\n    </p>\n\n  </article>\n</section>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/article.ejs"},"vd0cd4vcm68vpkaq":{"_id":"vd0cd4vcm68vpkaq","content":"<%\npage.category = ' ';\n%>\n<div class=\"archive-title\" >\n<h2 class=\"category-icon\"><%= __('categories') %></h2>\n<% site.categories.sort('name').each(function(item){ %>\n<a data-src=\"<%- config.root %><%- item.path %>\" href=\"#<%= item.name %>\" title=\"<%= item.name %>\"><%= item.name %><sup><%= item.posts.length %></sup></a>\n<% }); %>\n</div>\n<div id=\"main\" class=\"archive-part\">\n<div class=\"all-list-box\"></div>\n</div>\n\n","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/categories.ejs"},"yr0ju23gxd3o64m2":{"_id":"yr0ju23gxd3o64m2","content":"<div id=\"footer\" >\n\n</div>\n","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/footer.ejs"},"ygjd4bgnkn4jgkvn":{"_id":"ygjd4bgnkn4jgkvn","content":"<% if (theme.google_analytics){ %>\n<script type=\"text/javascript\">\n  var _gaq = _gaq || [];\n  _gaq.push(['_setAccount', '<%= theme.google_analytics %>']);\n  _gaq.push(['_trackPageview']);\n\n  (function() {\n    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;\n    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';\n    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);\n  })();\n</script>\n<% } %>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/google-analytics.ejs"},"k2pn3fo2zrkqn0h2":{"_id":"k2pn3fo2zrkqn0h2","content":"<% if (theme.google_analytics.enable){ %>\n<script type=\"text/javascript\">\n(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){\n  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),\n  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)\n})(window,document,'script','//www.google-analytics.com/analytics.js','ga');\nga('create', '<%= theme.google_analytics.id %>', '<%= theme.google_analytics.site %>');  \nga('send', 'pageview');\n</script>\n<% } %>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/google_analytics.ejs"},"t3kfxm4didj40tdm":{"_id":"t3kfxm4didj40tdm","content":"<!DOCTYPE HTML>\n<html <% if (config.language) { %>lang=\"<%= config.language %>\"<% } %>>\n<head>\n  <meta charset=\"UTF-8\">\n  <%\n  var title = page.title;\n\n  if (is_archive()){\n    title = 'Archives';\n\n    if (is_month()){\n      title += ': ' + page.year + '/' + page.month;\n    } else if (is_year()){\n      title += ': ' + page.year;\n    }\n  } else if (is_category()){\n    title = 'Category: ' + page.category;\n  } else if (is_tag()){\n    title = 'Tag: ' + page.tag;\n  }\n  %>\n    <title><% if (title){ %><%= title %> | <% } %><%= config.title %></title>\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=3, minimum-scale=1\">\n    <% if (config.author){ %>\n    <meta name=\"author\" content=\"<%= config.author %>\"><% } %>\n    <% if (page.description){ %>\n    <meta name=\"description\" itemprop=\"description\" content=\"<%= page.description %>\">\n    <% } else if (config.description&&(!is_post())){ %>\n    <meta name=\"description\" content=\"<%= config.description %>\">\n    <% } else if (page.excerpt){ %>\n    <meta name=\"description\" content=\"<%= strip_html(page.excerpt).replace(/^\\s*/, '').replace(/\\s*$/, '') %>\">\n    <% } else if (page.content){ %>\n    <meta name=\"description\" content=\"<%= strip_html(page.content).replace(/^\\s*/, '').replace(/\\s*$/, '').substring(0, 150) %>\">\n    <% } %>\n    <% if (page.keywords){ %><meta name=\"keywords\" content=\"<%= page.keywords %>\"><% } %>\n    <% if (theme.author.twitter){ %>\n    <meta name=\"twitter:card\" content=\"summary\" />\n    <meta name=\"twitter:site\" content=\"<%= theme.author.twitter %>\" />\n    <meta name=\"twitter:title\" content=\"<% if (title){ %><%= title %> | <% } %><%= config.title %>\" />\n      <% if (page.description){ %>\n        <meta name=\"twitter:description\" content=\"<%= page.description %>\" />\n      <% } %>\n    <% } %>\n    <% if (theme.rss){ %>\n    <link rel=\"alternative\" href=\"<%- theme.rss %>\" title=\"<%= config.title %>\" type=\"application/atom+xml\">\n    <% } %>\n    <% if (theme.favicon){ %>\n    <link rel=\"icon\" href=\"<%- config.root %><%- theme.favicon %>\">\n    <% } %>\n    <% if (theme.apple_icon){ %>\n    <link rel=\"apple-touch-icon\" href=\"<%- config.root %><%- theme.apple_icon %>\">\n    <link rel=\"apple-touch-icon-precomposed\" href=\"<%- config.root %><%- theme.apple_icon %>\">\n    <% } %>\n    <%- css('css/style') %>\n</head>\n","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/head.ejs"},"ib2yz43cyogoufpk":{"_id":"ib2yz43cyogoufpk","content":"<div>\n\t\t<% if\t(theme.imglogo.enable&&theme.imglogo.src){ %>\n\t\t\t<div id=\"imglogo\">\n\t\t\t\t<a href=\"<%- config.root %>\"><img src=\"<%- config.root %><%= theme.imglogo.src %>\" alt=\"<%= config.title %>\" title=\"<%= config.title %>\"/></a>\n\t\t\t</div>\n\t\t\t<% } %>\n\t\t\t<div id=\"textlogo\">\n\t\t\t\t<h1 class=\"site-name\"><a href=\"<%- config.root %>\" title=\"<%= config.title %>\"><%= config.title %></a></h1>\n\t\t\t\t<h2 class=\"blog-motto\"><% if (config.subtitle){ %><%= config.subtitle %><% } %></h2>\n\t\t\t</div>\n\t\t\t<div class=\"navbar\"><a class=\"navbutton navmobile\" href=\"#\" title=\"<%= __('menu') %>\">\n\t\t\t</a></div>\n\t\t\t<nav class=\"animated\">\n\t\t\t\t<ul>\n                    <ul>\n\t\t\t\t\t <% for (var i in theme.menu){ %>\n\t\t\t\t\t\t<li><a href=\"<%- theme.menu[i] %>\"><%= i %></a></li>\n\t\t\t\t\t<% } %>\n\t\t\t\t\t<li>\n\t\t\t\t\t<% if\t(theme.google_cse&&theme.google_cse.enable){ %>\n\t\t\t\t\t<form class=\"search\" action=\"<%- config.root %>search/index.html\" method=\"get\" accept-charset=\"utf-8\">\n\t\t\t\t\t\t<label>Search</label>\n\t\t\t\t\t\t<input type=\"text\" id=\"search\" autocomplete=\"off\" name=\"q\" maxlength=\"20\" placeholder=\"<%= __('search') %>\" />\n\t\t\t\t\t</form>\n\t\t\t\t\t<% } else if (theme.google_cse.enable){ %>\n\t\t\t\t\t<form class=\"search\" action=\"//google.com/search\" method=\"get\" accept-charset=\"utf-8\">\n\t\t\t\t\t\t<label>Search</label>\n\t\t\t\t\t\t<input type=\"text\" id=\"search\" name=\"q\" autocomplete=\"off\" maxlength=\"20\" placeholder=\"<%= __('search') %>\" />\n\t\t\t\t\t\t<input type=\"hidden\" name=\"q\" value=\"site:<%- config.url.replace(/^https?:\\/\\//, '') %>\">\n\t\t\t\t\t</form>\n\t\t\t\t\t<% } %>\n\t\t\t\t\t</li>\n                <li><div class=\"closeaside\"><a class=\"closebutton\" href=\"#\" title=\"<%= __('hidesidebar') %>\"></a></div></li>\n\n\t\t\t\t</ul>\n\t\t\t</nav>\t\n</div>\n","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/header.ejs"},"pyjw26mzfmlxsk9i":{"_id":"pyjw26mzfmlxsk9i","content":"<nav id=\"mobile-nav\">\n  <% for (var i in theme.menu){ %>\n    <a href=\"<%- theme.menu[i] %>\" class=\"mobile-nav-link\"><%= i %></a>\n  <% } %>\n</nav>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/mobile-nav.ejs"},"ax23o2qyfriwsd2e":{"_id":"ax23o2qyfriwsd2e","content":"<nav class=\"pagenav clearfix\">\n\t<% if (page.prev){ %>\n\t\t<a href=\"<%- config.root %><%- page.prev_link %>\" title=\"<%= __('prev') %>\" class=\"prevpage\"><%= __('prev') %></a>\n\t<% } %>\n\t<% if (page.next){ %>\n\t\t<a href=\"<%- config.root %><%- page.next_link %>\" title=\"<%= __('next') %>\" class=\"nextpage\"><%= __('next') %></a>\n\t<% } %>\n</nav>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/pagination.ejs"},"rx958wwtaq88aekq":{"_id":"rx958wwtaq88aekq","content":"<% if(theme.google_cse.enable) { %>\n<div  id=\"main\" class=\"page\">\n  <div id='cse' style='width:100%;text-align:center'>正在加载搜索结果，请稍等。</div>\n</div>\n<script src=\"http://www.google.com.hk/jsapi\" type=\"text/javascript\"></script>\n<script type=\"text/javascript\">\ngoogle.load('search', '1', {language : 'zh-CN',style: google.loader.themes.MINIMALIST});\ngoogle.setOnLoadCallback(function(){\n  var customSearchControl = new google.search.CustomSearchControl('<%= theme.google_cse.cx %>');\n  customSearchControl.setResultSetSize(google.search.Search.FILTERED_CSE_RESULTSET);\n  var options = new google.search.DrawOptions();\n  options.setAutoComplete(true);\n  customSearchControl.draw('cse', options);\n  var match = location.search.match(/q=([^&]*)(&|$)/);\n  if(match && match[1]){\n    var search = decodeURIComponent(match[1]); \n    customSearchControl.execute(search);\n  }\n},true);\n</script>\n<% } %>\n","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/search.ejs"},"7tbusi8j5z16zy3b":{"_id":"7tbusi8j5z16zy3b","content":"<% if( table&&(item.toc !== false) && theme.toc.aside){ %>\n  <div id=\"toc\" class=\"toc-aside\">\n  <strong class=\"toc-title\"><%= __('contents') %></strong>\n  <%- toc(item.content) %>\n  </div>\n<% } %>\n<div id=\"asidepart\">\n<div id=\"authorInfo\">\n\t<% if(theme.author_img_enable == true){ %>\n\t\t<div class=\"author-logo\"></div>\t\t\n\t<% } %>\n\t<% if(theme.author.intro_line1&&theme.author.intro_line2){ %>\n\t<section class=\"author-info\">\n\t\t<p> <%= theme.author.intro_line1 %></p>\n\t\t<p>\t<%= theme.author.intro_line2 %></p>\n\t</section>\n\t <% } %>\n\t<div class=\"social-font\" class=\"clearfix\">\n\t\t<% if(theme.author.weibo){ %>\n\t\t<a href=\"http://weibo.com/<%= theme.author.weibo %>\" target=\"_blank\" title=\"weibo\"></a>\n\t\t<% } %>\n\t\t<% if(theme.author.twitter){ %>\n\t\t<a href=\"https://twitter.com/<%=theme.author.twitter %>\" target=\"_blank\" title=\"twitter\"></a>\n\t\t<% } %>\n\t\t<% if(theme.author.github){ %>\n\t\t<a href=\"https://github.com/<%=theme.author.github %>\" target=\"_blank\" title=\"github\"></a>\n\t\t<% } %>\n\t\t<% if(theme.author.facebook){ %>\n\t\t<a href=\"https://www.facebook.com/<%=theme.author.facebook %>\" target=\"_blank\" title=\"facebook\"></a>\n\t\t<% } %>\n\t</div>\n</div>\n<aside class=\"clearfix\">\n\n<% theme.widgets.forEach(function(widget){ %>\n  <%- partial('../_widget/' + widget) %>\n<% }); %>\n</aside>\n</div>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/sidebar.ejs"},"ew476qzfnxqx9nf3":{"_id":"ew476qzfnxqx9nf3","content":"<%\npage.tag = ' ';\n%>\n<div class=\"archive-title\" >\n<h2 class=\"tag-icon\"><%= __('tags') %></h2>\n<% site.tags.sort('name').each(function(item){ %>\n<a data-src=\"<%- config.root %><%- item.path %>\" href=\"#<%= item.name %>\" title=\"<%= item.name %>\"><%= item.name %><sup><%= item.posts.length %></sup></a>\n<% }); %>\n</div>\n<div id=\"main\" class=\"archive-part\">\n<div class=\"all-list-box\"></div>\n</div>\n\n\n\n","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/tags.ejs"},"qqftx3w75offn9gy":{"_id":"qqftx3w75offn9gy","content":"<div id=\"main\" class=\"<%= item.layout %>\" itemscope itemprop=\"blogPost\">\n\t<article itemprop=\"articleBody\"> \n\t\t<%- partial('header') %>\n\t<div class=\"article-content\">\n\t\t<%- partial('gallery') %>\n\t\t<% if( table&&(item.toc !== false) && theme.toc.article){ %>\n\t\t<div id=\"toc\" class=\"toc-article\">\n\t\t\t<strong class=\"toc-title\"><%= __('contents') %></strong>\n\t\t<%- toc(item.content) %>\n\t\t</div>\n\t\t<% } %>\n\t\t<%- item.content %>  \n\t</div>\n\t\t<%- partial('footer') %>   \t       \n\t</article>\n\t<%- partial('pagination') %>\n\t<%- partial('comment') %>\n</div>  ","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/post/article.ejs"},"25yqi1tqj66fwg8n":{"_id":"25yqi1tqj66fwg8n","content":"<% if (item.tags && item.tags.length){ %>\n  <div class=\"article-tags\">\n  <% var tags = [];\n    item.tags.forEach(function(tag){\n      tags.push('<a href=\"' + config.root + tag.path + '\">' + tag.name + '</a>');\n    }); %>\n  <span></span> <%- tags.join('') %>\n  </div>\n<% } %>\n<% if (item.categories && item.categories.length){ %>\n<div class=\"article-categories\">\n  <span></span>\n  <%- list_categories(item.categories, {\n      show_count: false,\n      class: 'article-category',\n      style: 'none',\n      separator: '►'\n  }) %>\n</div>\n<% } %>\n\n","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/post/catetags.ejs"},"6nqswcy8pk9oxuaq":{"_id":"6nqswcy8pk9oxuaq","content":"<% if (theme.duoshuo.enable && page.comments){ %>\n\n\t<!-- 多说评论框 start -->\n\t<div class=\"ds-thread\" data-thread-key=\"请将此处替换成文章在你的站点中的ID\" data-title=\"请替换成文章的标题\" data-url=\"请替换成文章的网址\"></div>\n<!-- 多说评论框 end -->\n<!-- 多说公共JS代码 start (一个网页只需插入一次) -->\n<script type=\"text/javascript\">\nvar duoshuoQuery = {short_name:\"crispelite\"};\n\t(function() {\n\t\tvar ds = document.createElement('script');\n\t\tds.type = 'text/javascript';ds.async = true;\n\t\tds.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') + '//static.duoshuo.com/embed.js';\n\t\tds.charset = 'UTF-8';\n\t\t(document.getElementsByTagName('head')[0] \n\t\t || document.getElementsByTagName('body')[0]).appendChild(ds);\n\t})();\n\t</script>\n<!-- 多说公共JS代码 end -->\n<% } %>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/post/comment.ejs"},"ia9yw4clsokzassb":{"_id":"ia9yw4clsokzassb","content":"<footer class=\"article-footer clearfix\">\n<%- partial('catetags') %>\n<div class=\"article-share\" id=\"share\">\n<% if(theme.jiathis.enable){ %>\n  <%- partial('jiathis') %>\n<% } else { %>\n  <div data-url=\"<%- item.permalink %>\" data-title=\"<% if (item.title){ %><%= item.title %> | <% } %><%= config.title %>\" data-tsina=\"<%= theme.author.tsina %>\" class=\"share clearfix\">\n  </div>\n<% } %>\n</div>\n</footer>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/post/footer.ejs"},"7ami28hesq5zzr2o":{"_id":"7ami28hesq5zzr2o","content":"<% if (item.photos && item.photos.length){ %>\n<div class=\"article-gallery\">\n  <div class=\"article-gallery-photos\">\n    <% item.photos.forEach(function(photo, i){ %>\n      <a class=\"article-gallery-img fancybox\" href=\"<%- photo %>\" rel=\"gallery_<%= item._id %>\">\n        <img src=\"<%- photo %>\" itemprop=\"image\">\n      </a>\n    <% }) %>\n  </div>\n</div>\n<% } %>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/post/gallery.ejs"},"hvuminounfmsm2fl":{"_id":"hvuminounfmsm2fl","content":"<header class=\"article-info clearfix\">\n  <h1 itemprop=\"name\">\n    <% if(item.link) { %>\n      <a href=\"<%- item.link %>\" target=\"_blank\" title=\"<%= item.title %>\"><%= item.title %></a><% } else { %>\n      <a href=\"<%- config.root %><%- item.path %>\" title=\"<%= item.title %>\" itemprop=\"url\"><%= item.title %></a><% } %>\n  </h1>\n  <p class=\"article-author\">By\n    <% if (theme.author.google_plus){ %>\n      <a href=\"https://plus.google.com/<%- theme.author.google_plus %>?rel=author\" title=\"<%= config.author %>\" target=\"_blank\" itemprop=\"author\"><%= config.author %></a>\n    <% } else { %>\n      <a href=\"<%= config.url %>\" title=\"<%= config.author %>\"><%= config.author %></a>\n    <% } %></p>\n  <p class=\"article-time\">\n    <time datetime=\"<%= date_xml(item.date) %>\" itemprop=\"datePublished\"><%= item.date.format(config.date_format) %></time>\n    <% if (item.updated!=null) { %><%= __('updated') %>:<time datetime=\"<%= date_xml(item.updated) %>\" itemprop=\"dateModified\"><%= item.updated.format(config.date_format) %></time>\n    <% } %>\n  </p>\n</header>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/post/header.ejs"},"bre6byof8kp55spz":{"_id":"bre6byof8kp55spz","content":"<% if (theme.jiathis.enable){ %>\n<div class=\"jiathis_style\">\n    <span class=\"jiathis_txt\">分享到：</span>\n    <a class=\"jiathis_button_tsina\">新浪微博</a>\n    <a class=\"jiathis_button_weixin\">微信</a>\n    <a class=\"jiathis_button_twitter\">Twitter</a>\n    <a class=\"jiathis_button_evernote\">EverNote</a>\n    <a href=\"http://www.jiathis.com/share?uid=1501277\" class=\"jiathis jiathis_txt jiathis_separator jtico jtico_jiathis\" target=\"_blank\">更多</a>\n</div>\n<script type=\"text/javascript\" >\n    var jiathis_config={\n    data_track_clickback:true,\n    sm:\"copy,renren,cqq\",\n    pic:\"<%- item.photos %>\",\n    summary:\"\",\n    <% if (theme.jiathis.tsina){ %> ralateuid:{\"tsina\":\"<%= theme.jiathis.tsina %>\"},hideMore:false}\n    <% } %>\n  </script> \n<script type=\"text/javascript\" src=\"//v3.jiathis.com/code/jia.js?uid=\n<% if (theme.jiathis.id){ %><%= theme.jiathis.id %><% } %>\" charset=\"utf-8\"></script>      \n<% } %>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/post/jiathis.ejs"},"u2ykr6udrs5skmpd":{"_id":"u2ykr6udrs5skmpd","content":"<% if (page.prev || page.next){ %>\n<nav class=\"article-nav clearfix\">\n <% if (page.prev){ %>\n <div class=\"prev\" >\n <a href=\"<%- config.root %><%- page.prev.path %>\" title=\"<%= page.prev.title %>\">\n  <strong>PREVIOUS:</strong><br/>\n  <span>\n  <% if (page.prev.title){ %><%= page.prev.title %><% } else { %>(no title)<% } %></span>\n</a>\n</div>\n<% } %>\n<% if (page.next){ %>\n<div class=\"next\">\n<a href=\"<%- config.root %><%- page.next.path %>\"  title=\"<%= page.next.title %>\">\n <strong>NEXT:</strong><br/> \n <span><% if (page.next.title){ %><%= page.next.title %><% } else { %>(no title)<% } %>\n</span>\n</a>\n</div>\n<% } %>\n</nav>\n<% } %>","mtime":1402659139000,"source":"themes/Tinny/layout/_partial/post/pagination.ejs"}}}